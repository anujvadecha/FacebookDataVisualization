html
	head
		include mystyle.css
		script
	body
		h3 All the names stored in address book on first name basis:
		#table-wrapper
			table#contact_info
				tr
					th Name
					th Number
				each j,i in arr1
					tr
						td=arr1[i]
						td=arr2[i]
		h3 Phone interaction with the person on first name basis:
		#table-wrapper
			table#calls
				tr
					th Name
					th Count
					th Number
					th Duration_of_calls
					th Incoming_types
					th Outgoing_types
					th Missed_calls
				each j,i in arr3
					tr
						td=arr3[i]
						td=arr4[i]
						td=arr5[i]
						td=arr6[i]
						td=arr7[i]
						td=arr8[i]
						td=arr9[i]
		h3 Message Interaction 
		#table-wrapper
			table#messages
				tr
					th Name
					th Count_Inbox
					th Count_Sent
				each j,i in arr10
					tr
						td=arr10[i]
						td=countINBOX
						td=countSENT		
		h3 Search_History
		#table-wrapper
			table#messages
				tr
					th Name
					th NumberofTimessearched
					th Type
				each j,i in search
					tr
						td=search[i]
		h3 Facebook Message Interaction
		#table-wrapper
			table#messages
				tr
					th Name
					th Sent
					th Received
				tr
					each j,i in messages
						td=messages[i]
			a(href='/random') Random			